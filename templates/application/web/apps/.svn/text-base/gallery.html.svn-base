									{
										$('.popup-image').find('img').remove();
										$('.popup-image').append('<img src="'+basedomain+'/public_assets/'+strData.gambar_aspirasi_artist+'" id="contnetImg" gmbrc='+strData.gambar_aspirasi_artist+' />');
