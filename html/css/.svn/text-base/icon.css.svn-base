.iconMp3,.thePlayer .mejs-controls .mejs-play button,.thePlayer .mejs-controls .mejs-pause button,
.thePlayer .mejs-container .mejs-controls .mejs-volume-button button{
	background-image:url(../images/btn_player.png);
	background-repeat:no-repeat;
	display:block;
	width:25px; height:25px;
	cursor:pointer;
	-webkit-transition:none;
	-moz-transition: none;
	-o-transition: none;
	-ms-transition:none;
	transition:none;
}
.icon_play{
	background-position:-26px 0;
}
.current .icon_play,.thePlayer .mejs-controls .mejs-play button{
	background-position:0 0;
}
.icon_prev{
	background-position:-74px 0;
}
.icon_next{
	background-position:-48px 0px;
}
.icon_play:hover,.mejs-list tr:hover .icon_play{
	background-position:0 0;
}
.current .icon_play:hover,.thePlayer .mejs-controls .mejs-pause button{
	background-position:0  -25px;
}
.icon_prev:hover{
	background-position:-74px -25px;
}
.icon_next:hover{
	background-position:-48px -25px ;
}
.thePlayer .mejs-container .mejs-controls .mejs-volume-button button{
	background-image:url(../images/btn_player.png);
	background-repeat:no-repeat;
	background-position:-102px 0;
	margin:3px;
}
.thePlayer .mejs-container .mejs-controls .mejs-volume-button button:hover{
	background-position:-102px -25px;
}